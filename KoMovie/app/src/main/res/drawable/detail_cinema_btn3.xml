<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <corners android:bottomRightRadius="3dp" android:topRightRadius="3dp" />
            <solid android:color="#ff08b6f4" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:bottomRightRadius="3dp" android:topRightRadius="3dp" />
            <solid android:color="#fff" />
            <stroke android:width="1dp" android:color="#ff08b6f4" />
        </shape>
    </item>
</selector>